{
  "presets": [
    [
      "next/babel",
      {
        "preset-env": {
          "targets": {
            "browsers": "> 0.25%, not dead"
          },
          "bugfixes": true,
          "useBuiltIns": "usage",
          "corejs": 3.22
        }
      }
    ],
    "@babel/preset-flow",
    "@babel/preset-react"
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "alias": {
          "@": "."
        }
      }
    ]
  ]
}
