[ignore]
.*public/static.*
.*webpack-stats\.json.*
.*invalidPackageJson/package.json
.*/node_modules/.*/examples/.*
.*/node_modules/.staging/.*
.*/node_modules/.*/test/.*
.*/node_modules/fbjs/.*
.*/bower_components/.*

[include]

[libs]
interfaces/

[options]
module.system=node
suppress_type=$FlowIssue
suppress_comment= \\(.\\|\n\\)*\\flow:disable
esproposal.class_static_fields=ignore
esproposal.class_instance_fields=enable
esproposal.decorators=ignore

module.name_mapper='^AudioProcessingEvent$' ->  'empty/object'

[version]
0.80.0
